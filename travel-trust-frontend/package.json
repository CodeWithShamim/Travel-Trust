    "@stripe/stripe-js": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@wagmi/core": "^2.22.0",
    "@zama-fhe/oracle-solidity": "^0.2.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.18",
    "encrypted-types": "^0.0.4",
    "framer-motion": "^12.23.24",
    "framer-motion-3d": "^12.4.13",
    "mapbox-gl": "^3.15.0",
    "maplibre-gl": "^2.4.0",
    "media-chrome": "^4.14.0",
    "minimatch": "^10.0.3",
    "negotiator": "^0.6.3",
    "next": "15.5.4",
    "nextjs-toploader": "^3.9.17",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-map-gl": "^7.1.9",
    "react-player": "^3.3.3",
    "react-redux": "^9.2.0",
    "react-share": "^5.2.2",
    "scroll-carousel-react": "^1.0.0",
    "socket.io-client": "^4.8.1",
